        "@babel/preset-react": "^7.0.0",
        "axios": "^0.18",
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^23.6.0",
        "babel-loader": "^8.0.4",
        "babel-plugin-transform-class-properties": "^6.24.1",
